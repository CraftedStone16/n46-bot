{
  "permissions": {
    "READ_CONFIG": ["Moderator", "Bot Developer"],
    "EDIT_CONFIG": [ "Moderator", "Bot Developer"],
    "QUIT_BOT": ["Administrator", "Bot Developer"],
    "FETCH_HISTORY": ["Administrator", "Bot Developer"],
    "CHANGE_HISTORY": ["Administrator"],
    "PULL_UPDATES": ["Bot Developer"],
    "GREET_MEMBER": ["Greeter"],
    "UPDATE_GUILD": ["Moderator"],
    "MUTE_USER": ["Moderator"],
    "CHANGE_XP": ["Moderator", "Bot Developer"],
    "OVERRRIDE_IGNORE": ["Moderator"]
  },

  "greeting": {
    "on": true,
    "greetChannel": "introduction",
    "greeterTalkChannel": "greeters",
    "newMemberRole": "Newcomer",
    "greetMessage": "welcome"
  },

  "xp": {
    "on": true,
    "cooldownMinutes": 2,
    "xpIncreaseRangeLow": 1,
    "xpIncreaseRangeHigh": 2
  },

  "star": {
    "on": true,
    "starMinimum": 7,
    "starChannel": "featured",
    "ignore": ["selfies", "staff-chat"]
  },

  "ignore": {
    "channels": []
  }
}
